{
    "name": "airtemplate/airtemplate",
    "description": "A template engine for PHP devs. Lightweight, flexible and easy to use and extend in PHP.",
    "keywords": ["PHP", "templating", "template", "template engine", "micro-template", "developer"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Blaser"
        }
    ],
    "require": {
        "php": ">=5.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "phpunit/php-code-coverage": "~2.1",
        "squizlabs/php_codesniffer": "2.*",
        "mikey179/vfsStream": "~1",
        "satooshi/php-coveralls": "^1.0",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "AirTemplate\\": "src/"
        }
    }
}
